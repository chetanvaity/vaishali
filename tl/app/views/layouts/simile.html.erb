<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 //EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>

<head>
<title>Timeline</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

<script>
  Timeline_ajax_url="/javascripts/simile/timeline_ajax/simile-ajax-api.js";
  Timeline_urlPrefix='/javascripts/simile/timeline_js/';
  Timeline_parameters='bundle=true';
</script> 

<%= javascript_include_tag "jQuery/jquery-1.4.2.min.js" %>
<%= javascript_include_tag "rails.js" %>
<%= javascript_include_tag "simile/timeline_js/timeline-api.js" %>
<%= javascript_include_tag "jQueryTools/tooltip.min.js" %>

<%= stylesheet_link_tag "layout" %>
<%= stylesheet_link_tag "simile" %>
</head>

<body onload="onLoad();" onresize="onResize();">

<div id="contact" class="section">
  <div class="contents">
    <address>Hello user</address>
  </div>
</div>

<div id="header" class="section">
  <div class="contents">
    <div id="logo">
      <a href="/tl"><img alt="Timeline" src="/images/logo.gif"></a>
    </div>
  </div>
</div>

<div class="section preamble">
  <div class="flow">
    <div class="contents">
      <%= yield %>
    </div>
  </div>
</div>

</body>
</html>
